<project xmlns="http://maven.apache.org/POM/4.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<groupId>com.rean.mvn.test</groupId>
	<artifactId>snapshot-verifier-maven-plugin-test</artifactId>
	<version>0.0.1</version>
	<packaging>jar</packaging>
	<name>reminder-maven-plugin Maven Mojo</name>

	<dependencies>
		<dependency>
			<groupId>junit</groupId>
			<artifactId>junit</artifactId>
			<version>3.8.1</version>
			<scope>test</scope>
		</dependency>
	</dependencies>
	<build>
		<plugins>
			<plugin>
				<groupId>com.rean.mvn</groupId>
				<artifactId>snapshot-verifier-maven-plugin</artifactId>
				<version>0.0.1</version>
				<configuration></configuration>
				<executions>
                    <execution>
                        <goals>
                            <goal>verify</goal>
                        </goals>
                    </execution>
                </executions>
			</plugin>
		</plugins>
	</build>
</project>